{
  "version": 2,
  "builds": [
    { "src": "api/omdb.js", "use": "@vercel/node" },
    { "src": "api/auth.js", "use": "@vercel/node" },
    { "src": "api/cinematerial.js", "use": "@vercel/node" },
    { "src": "api/jobs-snapshot.js", "use": "@vercel/node" },
    { "src": "api/rss.js", "use": "@vercel/node" },
    { "src": "api/workingnomads.js", "use": "@vercel/node" },
    { "src": "api/headless-scrape-weworkremotely.js", "use": "@vercel/node" },
    { "src": "api/headless-scrape-multi.js", "use": "@vercel/node" },
    { "src": "api/headless-scrape-indeed.js", "use": "@vercel/node" },
    { "src": "api/headless-scrape-linkedin.js", "use": "@vercel/node" },
    { "src": "api/jobs-scraper-background.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/omdb(.*)", "dest": "/api/omdb.js" },
    { "src": "/api/auth", "dest": "/api/auth.js" },
    { "src": "/api/cinematerial(.*)", "dest": "/api/cinematerial.js" },
    { "src": "/api/jobs-snapshot", "dest": "/api/jobs-snapshot.js" },
    { "src": "/api/rss", "dest": "/api/rss.js" },
    { "src": "/api/workingnomads", "dest": "/api/workingnomads.js" },
    { "src": "/api/headless-scrape-weworkremotely", "dest": "/api/headless-scrape-weworkremotely.js" },
    { "src": "/api/headless-scrape-multi", "dest": "/api/headless-scrape-multi.js" },
    { "src": "/api/headless-scrape-indeed", "dest": "/api/headless-scrape-indeed.js" },
    { "src": "/api/headless-scrape-linkedin", "dest": "/api/headless-scrape-linkedin.js" },
    { "src": "/api/jobs-scraper-background", "dest": "/api/jobs-scraper-background.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
